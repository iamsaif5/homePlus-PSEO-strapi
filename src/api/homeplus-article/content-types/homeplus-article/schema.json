{
  "kind": "collectionType",
  "collectionName": "homeplus_articles",
  "info": {
    "singularName": "homeplus-article",
    "pluralName": "homeplus-articles",
    "displayName": "homeplus-article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::author.author",
      "mappedBy": "homeplus_article"
    },
    "blocks": {
      "type": "dynamiczone",
      "components": [
        "shared.quote",
        "shared.rich-text",
        "shared.media"
      ]
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "homeplus_articles"
    }
  }
}
