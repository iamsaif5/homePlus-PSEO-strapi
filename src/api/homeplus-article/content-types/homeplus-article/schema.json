{
  "kind": "collectionType",
  "collectionName": "homeplus_articles",
  "info": {
    "singularName": "homeplus-article",
    "pluralName": "homeplus-articles",
    "displayName": "homeplus-article"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "authors": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::author.author",
      "mappedBy": "homeplus_article"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "homeplus_articles"
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDescription": {
      "type": "text"
    },
    "excerpt": {
      "type": "text"
    },
    "aiDisclosure": {
      "type": "text"
    },
    "generationId": {
      "type": "string"
    },
    "qualityScore": {
      "type": "integer"
    },
    "totalWordCount": {
      "type": "integer"
    },
    "localDataPoints": {
      "type": "json"
    },
    "statistics": {
      "type": "json"
    },
    "faqs": {
      "type": "json"
    },
    "internalLinks": {
      "type": "json"
    },
    "publishStatus": {
      "type": "enumeration",
      "enum": [
        "draft",
        "review",
        "published"
      ]
    },
    "archetype": {
      "type": "string"
    },
    "trade": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::trade-collection.trade-collection",
      "inversedBy": "homeplus_articles"
    },
    "location": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::location-collection.location-collection",
      "inversedBy": "homeplus_articles"
    },
    "content": {
      "type": "blocks"
    }
  }
}
